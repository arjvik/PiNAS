# LetsEncrypt configuration for DNS-01 challenge
TRAEFIK_ACME_SERVER=https://acme-staging-v02.api.letsencrypt.org/directory
TRAEFIK_CLOUDFLARE_DNS_API_TOKEN=
# Traefik Pilot registration token
TRAEFIK_PILOT_TOKEN=
# Hostname for Traefik (intranet)
TRAEFIK_HOSTNAME=dev.zob.one
# Whitelisted IPs (internal networks, Cloudflare network)
TRAEFIK_IP_WHITELIST=192.168.1.0/24, 172.16.0.0/12, 103.21.244.0/22, 103.22.200.0/22, 103.31.4.0/22, 104.16.0.0/13, 104.24.0.0/14, 108.162.192.0/18, 131.0.72.0/22, 141.101.64.0/18, 162.158.0.0/15, 172.64.0.0/13, 173.245.48.0/20, 188.114.96.0/20, 190.93.240.0/20, 197.234.240.0/22, 198.41.128.0/17
# Hashed password (in Traefik middleware format, generated by htpasswd, escape $ signs with \$)
TRAEFIK_BASICAUTH_HASHED_PASSWORD=

# Cloudflared Tunnel configuration
# Base64-encoded cert.pem
CLOUDFLARED_CERTIFICATES=
# UUID for SSH Tunnel
CLOUDFLARED_SSH_TUNNEL_UUID=
# Tunnel credentials file for SSH tunnel
CLOUDFLARED_SSH_TUNNEL_CREDENTIALS=
# SSH endpoint for SSH tunnel
CLOUDFLARED_SSH_TUNNEL_ENDPOINT=ssh://docker.host:22/
# SSH cloudflared tunnel name
CLOUDFLARED_SSH_TUNNEL_NAME=

# SFTPGo envsubst script values (escape $ signs with \$ to appease Docker Compose gods!)
SFTPGO_USER1_USERNAME=
SFTPGO_USER1_PASSWORD=
SFTPGO_USER1_SSH_KEYS=
SFTPGO_USER2_USERNAME=
SFTPGO_USER2_PASSWORD=
SFTPGO_USER3_USERNAME=
SFTPGO_USER3_PASSWORD=
SFTPGO_USER4_USERNAME=
SFTPGO_USER4_PASSWORD=
SFTPGO_ADMIN_USERNAME=
SFTPGO_ADMIN_PASSWORD=

# PiHole configuration
PIHOLE_TIMEZONE=America/Chicago
# Web UI password (twice sha256 hashed, compute using `read pw; tr -d '\n' <<<"$pw" | sha256sum | cut -c-64 | tr -d '\n' | sha256sum`)
PIHOLE_HASHED_WEBPASSWORD=
# Hostname of NAS to override DNS response for
PIHOLE_HOSTNAME=${TRAEFIK_HOSTNAME}
# LAN IPv4 address of NAS to override DNS response with
PIHOLE_LAN_IPV4=
# LAN IPv6 address of NAS to override DNS response with
PIHOLE_LAN_IPV6=
# Upstream recursive DNS servers (seperated by ;)
PIHOLE_UPSTREAM_DNS=1.1.1.1;1.0.0.1
# Backup DNS for container
PIHOLE_BACKUP_DNS=1.1.1.1

# Apprise notification configuration
# Comma-seperated list of Apprise service urls (See https://github.com/caronc/apprise#productivity-based-notifications)
APPRISE_NOTIFICATION_URLS=ntfys://pinas-insecure

# IP Camera FFmpeg feed URLs (e.g. rtsp://user:password@host/)
IPCAMERA_FEED_1=
IPCAMERA_FEED_2=
IPCAMERA_FEED_3=
